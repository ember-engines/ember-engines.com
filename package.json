{
  "name": "ember-engines-website",
  "version": "1.0.0",
  "private": true,
  "description": "The official guides for Ember Engines",
  "repository": "https://github.com/ember-engines/ember-engines.com",
  "homepage": "https://ember-engines.netlify.app/",
  "license": "MIT",
  "author": "",
  "keywords": [
    "ember-addon",
    "ember-engines"
  ],
  "scripts": {
    "build": "ember build --environment=production",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember server",
    "test": "concurrently \"pnpm:lint\" \"pnpm:test:*\" --names \"lint,test:\" --prefixColors auto",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "deploy": "ADDON_DOCS_UPDATE_LATEST=true node_modules/.bin/ember deploy production"
  },
  "dependencies": {
    "@babel/core": "^7.27.1",
    "ember-cli-babel": "~8.2.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-template-imports": "^4.3.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.27.1",
    "@babel/plugin-proposal-decorators": "^7.27.1",
    "@ember/optional-features": "^2.2.0",
    "@ember/test-helpers": "^5.2.1",
    "@embroider/macros": "^1.17.2",
    "@embroider/test-setup": "^4.0.0",
    "@eslint/js": "^9.25.1",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^9.1.2",
    "ember-auto-import": "^2.10.0",
    "ember-cli": "~6.4.0",
    "ember-cli-addon-docs": "^10.1.0",
    "ember-cli-addon-docs-yuidoc": "^1.1.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.3",
    "ember-cli-deprecation-workflow": "^3.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-data": "^5.8.1",
    "ember-inflector": "^6.0.0",
    "ember-load-initializers": "^3.0.1",
    "ember-page-title": "^9.0.1",
    "ember-qunit": "^9.0.2",
    "ember-resolver": "^13.2.0",
    "ember-source": "~6.4.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^6.1.0",
    "ember-try": "^4.0.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "^12.5.0",
    "eslint-plugin-n": "^17.17.0",
    "eslint-plugin-qunit": "^8.1.2",
    "globals": "^15.15.0",
    "loader.js": "^4.7.0",
    "prettier": "^3.5.3",
    "prettier-plugin-ember-template-tag": "^2.0.5",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.4.0",
    "webpack": "^5.99.7"
  },
  "peerDependencies": {
    "ember-source": "^4.0.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": [
      "ember-cli-babel"
    ]
  }
}
